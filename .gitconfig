	name = Robert Horn
	email = rehorn09@gmail.com
[core]
	editor = vim
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset' --abbrev-commit
	co = checkout
	br = branch
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
